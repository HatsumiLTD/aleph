{
  "name": "@universalviewer/aleph",
  "version": "0.0.1-pre.22",
  "description": "Aleph",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "unpkg": "dist/aleph.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint:all": "prettier --write \"./src/**/*.{js,jsx,json,css,ts,tsx}\" \"!./src/assets/* \"",
    "build:dev": "npm run lint:all && stencil build --dev",
    "build": "npm run lint:all && stencil build --docs",
    "start": "stencil build --dev --serve",
    "start:ssl": "cross-env cp cert.pem ./www && cross-env cp key.pem ./www && http-server ./www -c-1 -o -S",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "netlify": "netlify deploy --dir=www --prod",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@edsilv/key-codes": "0.1.1",
    "@stencil/redux": "0.1.1",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@stencil/core": "0.18.1",
    "@types/aframe": "^0.8.3",
    "@types/jest": "24.0.13",
    "@types/puppeteer": "1.12.4",
    "@types/redux": "3.6.0",
    "@types/three": "0.103.2",
    "copyfiles": "2.1.0",
    "cross-env": "^5.2.0",
    "jest": "24.8.0",
    "jest-cli": "^24.8.0",
    "prettier": "^1.17.1",
    "prettier-check": "^2.0.0",
    "puppeteer": "^1.16.0",
    "typescript-tslint-plugin": "^0.4.0"
  },
  "license": "MIT"
}
